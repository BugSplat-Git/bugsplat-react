{
  "name": "bugsplat-react",
  "version": "0.1.0",
  "description": "bugsplat utilities for react",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest --testPathIgnorePatterns=/src/__integration_tests__/",
    "test:integration": "jest src/__integration_tests__/",
    "build": "tsc -p ./tsconfig.build.json",
    "prepare": "npm run build",
    "format": "prettier --write src/"
  },
  "keywords": [
    "bugsplat",
    "react"
  ],
  "author": "zmrl",
  "license": "ISC",
  "dependencies": {
    "bugsplat": "^7.1.3"
  },
  "devDependencies": {
    "@bugsplat/js-api-client": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.7.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
